@model IEnumerable<LocationMeasurement>
@{ 
    int i = 0;
}

<table class="table table-striped table-bordered table-condensed">
    <tr>
        <th>User</th>
        <th>Track</th>
        <th>ID</th>
        <th>Latitude</th>
        <th>Longitude</th>
        <th>Date Time Stamp</th>
        <th>Provider</th>
        <th>Fake Provider</th>
    </tr>
    @foreach (var m in Model) {
        if (i < 50) {
            <tr>
                <td>@m.UserID</td>
                <td>@m.TrackID</td>
                <td>@m.LocationMeasurementID</td>
                <td>@m.Latitude.ToString()</td>
                <td>@m.Longitude.ToString()</td>
                <td>@m.DateTimeStamp.ToString()</td>
                <td>@m.Provider</td>
                <td>@m.IsFromFakeProvider.ToString()</td>
            </tr>
        }
        i = i + 1;
    }
</table>

